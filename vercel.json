{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/ai_chat",
      "dest": "index.py"
    },
    {
      "src": "/send_message",
      "dest": "index.py"
    },
    {
      "src": "/get_updates",
      "dest": "index.py"
    }
  ]
}
